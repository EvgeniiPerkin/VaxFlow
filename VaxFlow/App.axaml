<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="VaxFlow.App"
             xmlns:local="using:VaxFlow"
             RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>
  
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://VaxFlow/Styles/MyStyles.axaml"/>
        <StyleInclude Source="avares://VaxFlow/Assets/Icons.axaml" />
    </Application.Styles>
</Application>